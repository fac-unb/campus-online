{
  "name": "campus-online-frontend",
  "description": "FAC-UNB campus online frontend web app",
  "homepage": "https://campus.kunst.cloud/",
  "version": "0.0.0-alpha",
  "private": true,
  "scripts": {
    "clean": "rm -rf public .cache",
    "dev": "gatsby develop --port \"${PORT:-5000}\" --open -H 0.0.0.0",
    "build": "gatsby build",
    "lint": "eslint './*.js' 'src' 'plugins' 'gatsby'",
    "format": "prettier --write \"{gatsby-*.js,src/**/*.js}\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{gatsby-,src/*}*.js": [
      "eslint",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@leonardodino/netlify-cms": "^1.8.1",
    "@reach/router": "^1.1.1",
    "deep-map": "^1.5.0",
    "gatsby": "^2.0.0-rc.4",
    "gatsby-image": "^2.0.0-rc.1",
    "gatsby-link": "^2.0.0-rc.2",
    "gatsby-plugin-lodash": "^3.0.1-rc.1",
    "gatsby-plugin-netlify": "^2.0.0-rc.1",
    "gatsby-plugin-netlify-cms": "^3.0.0-rc.1",
    "gatsby-plugin-nprogress": "^2.0.0-rc.1",
    "gatsby-plugin-react-helmet": "^3.0.0-rc.1",
    "gatsby-plugin-sharp": "^2.0.0-rc.2",
    "gatsby-plugin-styled-components": "^3.0.0-rc.1",
    "gatsby-react-router-scroll": "^2.0.0-rc.1",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^2.0.1-rc.1",
    "gatsby-remark-responsive-iframe": "^2.0.0-rc.1",
    "gatsby-source-filesystem": "^2.0.1-rc.1",
    "gatsby-transformer-remark": "^2.1.1-rc.1",
    "gatsby-transformer-sharp": "^2.1.1-rc.2",
    "lodash": "^4.17.5",
    "lru-cache": "^4.1.3",
    "polished": "^2.0.3",
    "prop-types": "^15.6.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-window-size": "^1.2.0",
    "recompose": "^0.27.1",
    "styled-components": "^3.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-plugin-styled-components": "^1.5.1",
    "eslint": "^5.4.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "gatsby-plugin-eslint": "^2.0.1",
    "lint-staged": "^7.1.2",
    "prettier": "^1.7.4"
  }
}
