{
  "name": "campus-online-functions",
  "description": "FAC-UNB campus online lambda functions",
  "homepage": "https://campus.kunst.cloud/",
  "version": "0.0.0-alpha",
  "private": true,
  "scripts": {
    "dev": "AWS_LAMBDA_JS_RUNTIME=nodejs8.10 netlify-lambda serve src -c functions.webpack.config.js",
    "lint": "eslint 'src'",
    "build": "AWS_LAMBDA_JS_RUNTIME=nodejs8.10 netlify-lambda build src -c functions.webpack.config.js",
    "precommit": "pretty-quick --staged && npm run lint",
    "format": "prettier --write \"{src/**/*.js}\""
  },
  "dependencies": {
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "netlify-lambda": "^0.4.0",
    "uglifyjs-webpack-plugin": "^1.3.0"
  }
}
